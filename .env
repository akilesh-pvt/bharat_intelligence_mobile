# Supabase Configuration
# Replace with your actual credentials
SUPABASE_URL=https://demo.supabase.co
SUPABASE_ANON_KEY=demo_anon_key_replace_with_actual
SUPABASE_SERVICE_ROLE_KEY=demo_service_role_key

# Google OAuth Configuration
GOOGLE_CLIENT_ID_WEB=demo_web_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_ID_ANDROID=demo_android_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_ID_IOS=demo_ios_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET_WEB=demo_client_secret

# Project Configuration
PROJECT_ID=bharat-intelligence-demo
APP_NAME=Bharat Intelligence Admin
APP_DISPLAY_NAME=BI Admin Dashboard
APP_VERSION=1.0.0
BUILD_NUMBER=1
ENVIRONMENT=development
PACKAGE_NAME=com.bharatintelligence.admin
BUNDLE_ID=com.bharatintelligence.admin

# API Configuration
API_BASE_URL=https://demo.supabase.co/rest/v1
API_TIMEOUT=30000
API_RETRY_COUNT=3

# Security Configuration
SESSION_TIMEOUT=86400
MAX_LOGIN_ATTEMPTS=5
JWT_EXPIRY=7200
REFRESH_TOKEN_EXPIRY=604800
ENCRYPTION_KEY=demo_32_character_key_replace_me

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_BIOMETRIC_AUTH=false
ENABLE_OFFLINE_MODE=true
ENABLE_DARK_MODE=true
DEBUG_MODE=true
ENABLE_LOGGING=true

# File Upload Configuration
MAX_FILE_SIZE_MB=10
ALLOWED_IMAGE_FORMATS=jpg,jpeg,png,webp,heic
STORAGE_BUCKET_UPLOADS=admin-uploads
IMAGE_COMPRESSION_QUALITY=80

# Maps & Location Configuration
GOOGLE_MAPS_API_KEY=demo_maps_api_key
LOCATION_ACCURACY_METERS=10
DEFAULT_MAP_ZOOM=15

# Business Logic Configuration
FUEL_RATE_PER_KM=5.50
OVERTIME_RATE_MULTIPLIER=1.5
TASK_DEADLINE_WARNING_DAYS=3
AUTO_APPROVE_ALLOWANCE_THRESHOLD=500

# Development & Testing
MOCK_DATA=false
ENABLE_INSPECTOR=true
TEST_USER_EMAIL=test@bharatintelligence.com
TEST_MODE=false

# Platform Specific
ANDROID_MIN_SDK=21
ANDROID_TARGET_SDK=34
IOS_DEPLOYMENT_TARGET=12.0

# Localization
DEFAULT_LOCALE=en_IN
CURRENCY_CODE=INR
CURRENCY_SYMBOL=â‚¹
DATE_FORMAT=dd/MM/yyyy